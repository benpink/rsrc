    "@theme-ui/style-guide": "0.2.44",
    "core-js": "3.3.2",
    "gh-pages": "2.1.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.0.6",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "rebass": "4.0.6",
    "regenerator-runtime": "0.13.3",
    "remark-slug": "5.1.2",
    "rimraf": "3.0.0",
