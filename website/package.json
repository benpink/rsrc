{
  "name": "rsrc-website",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf {.next,out,rsrc}",
    "build": "yarn clean && next build",
    "export": "yarn build && next export",
    "preview": "yarn export && ln -sf out rsrc && npx serve",
    "publish-docs": "yarn export && touch out/.nojekyll && gh-pages -t -d out"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@emotion/core": "10.0.28",
    "@mdx-js/loader": "1.6.6",
    "@mdx-js/react": "1.6.6",
    "@next/mdx": "9.4.4",
    "@theme-ui/prism": "0.3.0",
    "core-js": "3.6.5",
    "gh-pages": "3.1.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "^9.3.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rebass": "4.0.7",
    "regenerator-runtime": "0.13.5",
    "remark-slug": "6.0.0",
    "rimraf": "3.0.2",
    "theme-ui": "0.3.1"
  },
  "dependencies": {
    "rsrc": "^1.1.2"
  }
}
