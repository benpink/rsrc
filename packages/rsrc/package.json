{
  "name": "rsrc",
  "author": "Signal Sciences",
  "description": "React components for managing async fetch state",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "resource",
    "fetch",
    "cache"
  ],
  "homepage": "https://github.com/signalsciences/rsrc",
  "bugs": {
    "url": "https://github.com/signalsciences/rsrc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signalsciences/rsrc.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "scripts": {
    "build": "yarn clean && rollup -c",
    "clean": "rm -rf dist",
    "dev": "yarn build --watch",
    "flow": "flow",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "jest": "jest",
    "test": "yarn flow && yarn lint && yarn jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-signalsciences"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-signalsciences"
    ]
  },
  "jest": {
    "preset": "jest-preset-signalsciences",
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "dependencies": {
    "query-string": "^6.1.0",
    "url-template": "^2.0.8"
  },
  "peerDependencies": {
    "react": "^16.3",
    "react-dom": "^16.3"
  },
  "devDependencies": {
    "babel-preset-signalsciences": "^0.0.1",
    "eslint-config-signalsciences": "^0.0.1",
    "flow-bin": "^0.88.0",
    "flow-typed": "^2.5.1",
    "jest-fetch-mock": "^2.1.0",
    "jest-preset-signalsciences": "^0.0.2",
    "react": "^16.3",
    "react-dom": "^16.3",
    "react-testing-library": "^5.4.4",
    "rollup-config-signalsciences": "^0.0.1",
    "rollup-plugin-bundle-size": "^1.0.3"
  }
}
